{
  "name": "gorgon.exchange",
  "version": "1.0.0",
  "description": "The site that runs gorgon.exchange",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "compile": "mkdir -p dist && NODE_ENV=production browserify -g envify -g reactify app/app.jsx -o dist/gorgon.dev.js",
    "css": "stylus app/gorgon.styl",
    "dev": "mkdir -p dist && NODE_ENV=development browserify -g envify -g reactify app/app.jsx -o dist/gorgon.dev.js",
    "production": "NODE_ENV=production forever start index.js",
    "restart": "NODE_ENV=production forever restart 0"
  },
  "keywords": [
    "gorgon",
    "exchange",
    "chest"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "dependencies": {
    "async": "1.0.x",
    "connected": "0.0.x",
    "diagnostics": "1.0.x",
    "ejs": "2.3.x",
    "emits": "3.0.x",
    "eventemitter3": "1.0.x",
    "expirable": "0.1.x",
    "express": "4.12.x",
    "moment": "2.10.x",
    "notifyjs": "1.2.5",
    "object-assign": "2.0.x",
    "one-time": "0.0.x",
    "primus": "3.1.x",
    "primus-emit": "0.1.x",
    "request": "2.57.x",
    "serve-static": "1.9.x",
    "snoocore": "3.0.x",
    "ws": "0.7.x"
  },
  "devDependencies": {
    "browserify": "10.1.x",
    "envify": "3.4.x",
    "forever": "*",
    "react": "0.13.x",
    "reactify": "1.1.x",
    "stylus": "0.51.x",
    "uglifyify": "3.0.x"
  }
}
