{
  "name": "gorgon.exchange",
  "version": "1.0.0",
  "description": "The site that runs gorgon.exchange",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "compile": "mkdir -p dist && NODE_ENV=production browserify -g envify -g reactify app/app.jsx -o dist/gorgon.dev.js",
    "css": "stylus app/gorgon.styl",
    "dev": "mkdir -p dist && NODE_ENV=development browserify -g envify -g reactify app/app.jsx -o dist/gorgon.dev.js",
    "production": "NODE_ENV=production forever start index.js",
    "restart": "NODE_ENV=production forever restart 0"
  },
  "keywords": [
    "gorgon",
    "exchange",
    "chest"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "dependencies": {
    "async": "~2.1.4",
    "connected": "0.0.x",
    "diagnostics": "~1.1.0",
    "ejs": "~2.5.5",
    "emits": "3.0.x",
    "eventemitter3": "~2.0.2",
    "expirable": "0.1.x",
    "express": "~4.14.0",
    "moment": "~2.17.1",
    "notifyjs": "2.0.3",
    "object-assign": "~4.1.0",
    "one-time": "0.0.x",
    "primus": "~6.0.9",
    "primus-emit": "~1.0.0",
    "request": "~2.79.0",
    "serve-static": "~1.11.1",
    "snoocore": "~3.3.1",
    "ws": "~1.1.1"
  },
  "devDependencies": {
    "browserify": "~13.2.0",
    "envify": "~4.0.0",
    "forever": "*",
    "react": "~15.4.1",
    "reactify": "1.1.x",
    "stylus": "~0.54.5",
    "uglifyify": "3.0.x"
  }
}
